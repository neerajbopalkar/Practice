<Window x:Class="Shapes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Canvas>
        <Rectangle Height="100" Width="50" Fill="Blue"
                   Canvas.Top="50" Canvas.Left="50"
                   Stroke="Yellow" StrokeThickness="5"
            ></Rectangle>

        <Ellipse Height="100" Width="50" Fill="Blue"
                   Canvas.Top="50" Canvas.Left="150"
                   Stroke="Red" StrokeThickness="2"
            >

        </Ellipse>
        <Line X1="0" Y1="0" X2="30" Y2="30" Stroke="Orange" StrokeThickness="10"
            >

        </Line>
        <Polyline Points="100,100,200,150,125,175,175,250" Stroke="Blue" StrokeThickness="5"
            ></Polyline>

        <Polygon Fill="Green" Points="100,100,200,150,125,175,175,250" Stroke="Blue" StrokeThickness="5"
            ></Polygon>
    </Canvas>
</Window>
